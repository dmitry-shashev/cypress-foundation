{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "lib": ["es5", "dom"],
    "types": ["cypress", "node"],
    "sourceMap": false,
    "useUnknownInCatchVariables": false,
    "strictNullChecks": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitOverride": true
  },
  "include": ["cypress/**/*.ts"]
}
